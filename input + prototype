print('(0<=x,y<=10)')
print('출발지의 좌표를 입력해주세요')
a = input().split()
print('도착지의 좌표를 입력해주세요')
b = input().split()

n = []
n.append(a[0])
n.append(b[0])
m = []
m.append(a[1])
m.append(b[1])

for i in n :
    if int(i) < 0 or int(i) > 10 :
        print('0')
for j in m :
    if int(j) < 0 or int(j) > 10 :
        print('0')
        
c = []
d = []
for i in range (int(a[0]), int(b[0]) + 1) :
    c.append(int(0))
for i in range (int(a[1]), int(b[1]) + 1) :
    d.append(int(0))
print(c)
print(d)
